      "post": {
        "tags": [
          "Student Authentication Page"
        ],
        "description": "Endpoint to sign in a specific student and have access to their data",
        "parameters": [
          {
            "name": "obj",
            "in": "body",
            "description": "Student Students login details",
            "required": true,
            "type": "object",
            "schema": {
              "type": "object",
              "properties": {
                "studentId": {
                  "type": "string",
                  "example": "011273Vb"
                },
                "password": {
                  "type": "string",
                  "example": "password_demo_123"
                }
              },
              "required": [
                "studentId",
                "password"
              ]
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Login successfully",
            "schema": {
              "type": "object",
              "properties": {
                "message": {
                  "type": "string",
                  "example": "student successfully login"
                },
                "data": {
                  "type": "object",
                  "properties": {
                    "token": {
                      "type": "string",
                      "example": "token"
                    },
                    "student": {
                      "type": "object",
                      "properties": {
                        "_id": {
                          "type": "string",
                          "example": "0030303"
                        },
                        "studentId": {
                          "type": "string",
                          "example": "49494"
                        },
                        "First Name": {
                          "type": "string",
                          "example": "ade"
                        },
                        "Last Name": {
                          "type": "string",
                          "example": "Yemi"
                        }
                      }
                    }
                  }
                }
              },
              "xml": {
                "name": "main"
              }
            }
          },
          "400": {
            "description": "Bad Request"
          },
          "404": {
            "description": "user not found",
            "schema": {
              "type": "object",
              "properties": {
                "error ": {
                  "type": "string",
                  "example": "invalid Student Id "
                }
              },
              "xml": {
                "name": "main"
              }
            }
          },
          "405": {
            "description": "Password error",
            "schema": {
              "type": "object",
              "properties": {
                "error ": {
                  "type": "string",
                  "example": "student Id and Password not match, incorrect password"
                }
              },
              "xml": {
                "name": "main"
              }
            }
          }
        }
      }
    },
